<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.xinjia.presentation.tablecontroller.TableLayoutController">
   <children>
      <TableView fx:id="emailDataTable" onDragDetected="#onDragDetected" prefHeight="506.0" prefWidth="680.0" stylesheets="@../styles/tableview.css">
        <columns>
          <TableColumn fx:id="fromColumn" prefWidth="75.0" text="%from" />
          <TableColumn fx:id="subjectColumn" prefWidth="75.0" text="%subject" />
            <TableColumn fx:id="dateColumn" prefWidth="75.0" text="%date" />
            <TableColumn fx:id="toColumn" prefWidth="75.0" text="%to" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
